<h1>座席予約キャンセル</h1>

<div class="row">
    <form method="post">
        <div class="form-group">
            <label for="id_payment_no">予約番号</label>
            <input type="text" name="payment_no" id="id_payment_no" class="form-control" value="<%- form.fields.payment_no.value %>">
        </div>

        <div class="form-group">
            <label for="id_tel">電話番号下4ケタ</label>
            <input type="text" name="tel" id="id_tel" class="form-control" value="<%- form.fields.tel.value %>">
        </div>

        <input type="submit" class="btn btn-primary btn-lg btn-block">
    </form>
</div>
