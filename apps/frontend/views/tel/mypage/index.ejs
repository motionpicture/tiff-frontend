<input type="hidden" name="purchaser_group_name_<%- ReservationUtil.PURCHASER_GROUP_CUSTOMER %>" value="一般">
<input type="hidden" name="purchaser_group_name_<%- ReservationUtil.PURCHASER_GROUP_TEL %>" value="電話">
<input type="hidden" name="purchaser_group_name_<%- ReservationUtil.PURCHASER_GROUP_WINDOW %>" value="当日">
<input type="hidden" name="purchaser_group_name_<%- ReservationUtil.PURCHASER_GROUP_SPONSOR %>" value="外部関係者">
<input type="hidden" name="purchaser_group_name_<%- ReservationUtil.PURCHASER_GROUP_MEMBER %>" value="先行予約">

<div class="wrapper-searchform">
    <form class="search-form" action="<%- url('tel.mypage.search') %>">
    <input type="hidden" name="limit" value="20">

    <table class="table">
        <tbody>
            <tr>
                <td>
                    予約区分<br>
                    <div class="form-group">
                        <select class="form-control" name="purchaser_groups">
                            <option value="">---</option>
                            <option value="<%- ReservationUtil.PURCHASER_GROUP_CUSTOMER %>">一般</option>
                            <option value="<%- ReservationUtil.PURCHASER_GROUP_SPONSOR %>">外部関係者</option>
                            <option value="<%- ReservationUtil.PURCHASER_GROUP_MEMBER %>">先行予約</option>
                            <option value="<%- ReservationUtil.PURCHASER_GROUP_TEL %>">電話</option>
                            <option value="<%- ReservationUtil.PURCHASER_GROUP_WINDOW %>">当日</option>
                        </select>
                    </div>
                </td>
                <td>
                    予約日<br>
                    <div class="form-group">
                        <select class="form-control" name="purchased_day">
                            <option value="">---</option>
                            <% for (let i=0; i<30; i++) { %>
                            <% let _moment =  moment().add(-i, 'days') %>
                            <option value="<%- _moment.format('YYYYMMDD') %>"><%- _moment.format('MM/DD') %></option>
                            <% } %>
                        </select>
                    </div>
                </td>
                <td>
                    メールアドレス<br>
                    <div class="form-group">
                        <input type="text" class="form-control" name="email">
                    </div>
                </td>
                <td>
                    電話番号<br>
                    <div class="form-group">
                        <input type="text" class="form-control" name="tel">
                    </div>
                </td>
                <td>
                    氏名(カタカナ)<br>
                    <div class="form-inline">
                        <div class="form-group">
                            <input type="text" class="form-control" name="purchaser_name">
                        </div>
                        <div class="form-group">
                            <a class="btn" href="javascript:void(0)"><span><%- __('Button.Search') %></span></a>
                        </div>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    </form>
</div>

<p class="row">About <span class="total-count"></span> results</p>

<div class="wrapper-reservations">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th><input type="checkbox" class="check-all"></th>
                <th><%- __('Label.PaymentNo') %></th>
                <th>予約区分</th>
                <th>予約日時</th>
                <th><%- __('Label.Email') %></th>
                <th><%- __('Label.Tel') %></th>
                <th>氏名</th>
                <th><%- __('Label.FilmName') %> / <%- __('Label.PerformanceTime') %> / <%- __('Label.PerformancePlace') %></th>
                <th><%- __('Label.SeatCode') %></th>
                <th></th>
            </tr>
        </thead>
        <tbody id="reservations">

        </tbody>
    </table>
</div>



<div class="wrapper-controls">

    <div class="form-inline">
        <div class="form-group">
            <select class="form-control" name="action">
                <option value=""><%- __('Label.Actions') %></option>
                <option value="cancel"><%- __('Label.Cancel') %></option>
                <option value="print"><%- __('Label.Print') %></option>
            </select>
        </div>
        <div class="form-group">
            <p class="btn action-to-reservations"><span><%- __('Label.Apply') %></span></p>
        </div>
    </div>

    <div class="navigation">
    </div>

</div>





<div class="modal fade cancel-reservation-confirm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>

            <div class="modal-body">
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal"><%- __('Button.No') %></button>
                <button type="button" class="btn btn-primary execute-cancel"><%- __('Button.Yes') %></button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade cancel-reservation-complete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>

            <div class="modal-body">
                <%- __('Label.CompleteCancelReservations') %>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal"><%- __('Button.Close') %></button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade loading" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <div class="text-center"><%- __('Message.Searching') %></div>
            </div>
        </div>
    </div>
</div>

<input type="hidden" name="urlCancel" value="<%- url('tel.cancel.execute') %>">

<script src="/js/tel/mypage.js"></script>

